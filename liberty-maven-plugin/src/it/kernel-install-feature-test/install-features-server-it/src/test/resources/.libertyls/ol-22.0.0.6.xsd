                    <ext:label>Group ID map</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupMemberIdMap" use="optional" default="memberOf:member" type="xsd:string"> 
            <xsd:annotation> 
